- title "Project"

%p
  %strong Name:
  =h @project.name
%p
  %strong Description:
  = h @project.description

%p
  %strong Start:
  = h @project.start

%p
  %strong Finish:
  = h @project.finish

%p
  = link_to "Edit", edit_project_path(@project)
  |
  = link_to "Destroy", @project, :confirm => 'Are you sure?', :method => :delete
  |
  = link_to "View All", projects_path